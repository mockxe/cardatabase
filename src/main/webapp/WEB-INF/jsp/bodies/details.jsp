<div class="table">
	<table>
		<tr>
			<th>attribute</th>
			<th>value</th>
		</tr>
		<tr>
			<td>Car brand</td>
			<td>${brand}</td>
		</tr>
		<tr>
			<td>Engine type</td>
			<td>${engine}</td>
		</tr>
		<tr>
			<td>Displacement</td>
			<td>${displacement} cm<sup>3</sup>
			</td>
		</tr>
		<tr>
			<td>Date of first registration</td>
			<td>${date}</td>
		</tr>
		<tr>
			<td>Number of doors</td>
			<td>${doors}</td>
		</tr>
		<tr>
			<td>Color</td>
			<td bgcolor="${color}"></td>
		</tr>
		<tr>
			<td>Total weight</td>
			<td>${weight} kg</td>
		</tr>
		<tr>
			<td>Chassis number</td>
			<td>${chassis}</td>
		</tr>
		<tr>
			<td>Pollutant class</td>
			<td>Euro ${pollutant}</td>
		</tr>
		<tr>
			<td>Description</td>
			<td><textarea rows="5" cols="60" readonly>${description}</textarea></td>
		</tr>
	</table>
</div>